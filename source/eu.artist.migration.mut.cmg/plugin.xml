<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Model Understanding Tool"
            id="eu.artist.migration.mut.category">
      </category>
      <command
            name="Generate a component model"
            category="eu.artist.migration.mut.category"
            categoryId="eu.artist.migration.mut.category"
            id="eu.artist.migration.mut.modelcomponentgenerator.commands.generateComponentModelCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="eu.artist.migration.mut.modelcomponentgenerator.commands.generateComponentModelCommand"
            class="eu.artist.migration.mut.cmg.handlers.GenerateComponentModelHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <menu
               label="Model Understanding Toolbox"
               id="eu.artist.migration.mut.menu.main">
            <command
                  commandId="eu.artist.migration.mut.modelcomponentgenerator.commands.generateComponentModelCommand"
                  mnemonic="M"
                  id="eu.artist.migration.mut.modelcomponentgenerator.menus.generateComponentModelCommand">
               <visibleWhen>
                  <with
                        variable="activeMenuSelection">
                     <iterate
                           ifEmpty="false">
                        <adapt
                              type="org.eclipse.core.resources.IResource">
                           <test
                                 value="*.uml"
                                 property="org.eclipse.core.resources.name">
                           </test>
                        </adapt>
                     </iterate>
                  </with>
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
   </extension>
